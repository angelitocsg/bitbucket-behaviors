{
    "manifest_version": 2,
    "name": "Bitbucket Extension",
    "version": "0.0.1",
    "description": "Changes Bitbucket defaults behaviors",
    "icons": {
        "128": "128.png"
    },
    "permissions": [
        "notifications"
    ],
    "browser_action": {
        "default_icon": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://bitbucket.org/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}